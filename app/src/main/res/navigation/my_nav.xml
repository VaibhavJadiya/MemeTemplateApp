<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.printoverit.machinetestapp.fragments.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_memeDetailsActivity"
            app:destination="@id/memeDetailsActivity" />
    </fragment>
    <activity
        android:id="@+id/memeDetailsActivity"
        android:name="com.printoverit.machinetestapp.MemeDetailsActivity"
        android:label="activity_meme_details"
        tools:layout="@layout/activity_meme_details" >
        <argument
            android:name="data"
            app:argType="com.printoverit.machinetestapp.dataclasses.Meme" />
    </activity>
</navigation>